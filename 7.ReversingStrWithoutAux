#include <stdio.h>

int main()
{
    char name[]="Welcome";
    
    char temp;
    int i,j;
    for(j=0;name[j]!='\0';j++)
    {
        
    }
    j=j-1;
    for(i=0;i<j;i++,j--)
    {
        temp=name[i];
        name[i]=name[j];
        name[j]=temp;
        
    }
   
    printf("After reversing the string was %s",name);
   

   
}

